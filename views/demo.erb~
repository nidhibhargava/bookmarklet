<script type="text/html" id="toggleTemplate">
	<form action="/" method="post"><input type="submit" class="go" value="go"></form><input type="submit" class="back" value="back">
</script>
<script>
	$(function(){

			var toggleTemplate=$('#toggleTemplate').html();


			$('.outer').click(function () {
				var $me=$(this);
				var theOG=$me.html();

				$me.html(toggleTemplate);
					$('.result').text('Edit Clicked');

				$me.find('.go').click(function () {
					$('.result').text('Off We Go');

					//bubbling
				});

				$me.find('.back').click(function () {
					$me.html(theOG);
					$('.result').text('Went Back');

					//no bubbling
					return false;
				});

			});


	});
</script>


<div class="outer">
	<input type="submit" class="edit" value="edit">
</div>

<div class="result">Start</div>

